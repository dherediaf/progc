#include <iostream>
#include <string>

/* (?:) este condicional se usa para lo mismo que el if-else para imprimir
 en la pantalla, su estructura es :

   ((condicion)? opcion if : opcion else )  

 */

using namespace std;

int main()
{
  string a="";

  cout << "Escriba el usuario del sia del simpatico Diego Heredia\n";

  cin >> a;

  cout << ( (a=="dherediaf") ? "Correcto\n" : "Incorrecto, no conoces a diego?\n");
  
  return 0;
}
