#include <iostream>
using namespace std;

int main()
{
  int a;
  int b=0;
  cout << "Escriba un numero para verificar si es primo\n";
  cin >> a;

  for(int b=1; b<=a; b=++b)
    {
      if(a%b==0)
	{
	  b=++b;
	}
    }
  if(b==2)
    {
      cout << a << " es primo\n";
    }
  else
    {
      cout << a << " no es primo\n";
    }


  return 0;
}
